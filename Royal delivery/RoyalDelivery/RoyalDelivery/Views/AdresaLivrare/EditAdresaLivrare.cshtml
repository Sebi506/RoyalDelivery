@model RoyalDelivery.Models.DBObjects.AdresaLivrareModel

@{
    ViewData["Title"] = "EditAdresaLivrare";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Modificare adresa livrare </h1>

<hr />
<div class="row">
