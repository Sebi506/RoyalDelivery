            </div>
            <div class="form-group">
                <label asp-for="Strada" class="control-label"></label>
                <input asp-for="Strada" class="form-control" />
                <span asp-validation-for="Strada" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Numar" class="control-label"></label>
                <input asp-for="Numar" class="form-control" />
                <span asp-validation-for="Numar" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InformatiiAditionale" class="control-label"></label>
                <input asp-for="InformatiiAditionale" class="form-control" />
                <span asp-validation-for="InformatiiAditionale" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
